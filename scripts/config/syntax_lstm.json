        "subtasks": [
          "SP",
          "PS",
          "S",
          "P",
          "SPP",
          "PSS",
          "SPPP",
          "PSSS"
        ]
      },
      "lakretz": {
        "path": "../lm_data/corpora/downstream/lakretz",
